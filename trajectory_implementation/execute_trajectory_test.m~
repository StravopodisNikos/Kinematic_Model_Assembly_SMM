% Ovidius robot spaecifications 
qd1_max = 10; %[rad/s]
qd2_max = 20; %[rad/s]
qd3_max = 20; %[rad/s]
qdd1_max = 100; %[rad/s2]
qdd2_max = 200; %[rad/s2]
qdd3_max = 200; %[rad/s2]

% Define a C-space trajectory using a 5th order polynomial interpolation
% as specified @ : https://www.mathworks.com/help/robotics/ref/quinticpolytraj.html

% wpts must be nxp array
way_pts = [1.5708 1.1  0.84 0.15 -0.7854 -1.5708;...   %q1
           1.5708 1.4  1.35 0.58  0.7854  1.5708 ;...   %q2
           1.5708 1.26 2.14 2.45  1.5708  0 ];...  %q2
% tpts must be px1 vector        
time_pts = [0 0.1 0.25 0.5 0.75]';

% sampling times is a mx1 vector
time_sample = time_pts(1):0.01:time_pts(length(time_pts));

